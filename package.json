        "fluid-binder": "1.1.2",
        "fluid-couch-config": "git+https://github.com/BlueSlug/fluid-couch-config.git",
        "fluid-express-user": "2.0.2",
        "fluid-handlebars": "2.1.4",
        "fluid-json-schema": "2.1.6",
        "fluid-location-bar-relay": "1.0.7",
        "fs-extra": "9.1.0",
